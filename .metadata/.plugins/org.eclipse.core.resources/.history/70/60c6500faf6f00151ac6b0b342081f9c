from django.db import models
from django.utils import timezone
 
import datetime

class Equipe(models.Model):
    nome = models.Charfield
  
  
  '''
  equipe_id serial NOT NULL,
  equipe_nome character varying(50) NOT NULL,
  equipe_usuariorp character varying(50) NOT NULL,
  equipe_data_registro date NOT NULL,
  CONSTRAINT equipes_pkey PRIMARY KEY (equipe_id)
  '''



